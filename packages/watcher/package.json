    "@types/pluralize": "^0.0.29",
    "@types/puppeteer": "^1.19.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cucumber": "^5.1.0",
    "faker": "^4.1.0",
    "imgur": "^0.3.1",
