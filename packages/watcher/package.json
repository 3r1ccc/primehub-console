{
  "name": "@infuseai/watcher",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development ts-node src/bin/www.ts",
    "start:lib": "NODE_ENV=development node lib/bin/www",
    "start:prod": "NODE_ENV=production node lib/bin/www",
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\" \"test/*\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.json --pretty",
    "build:prod": "npm run clean && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.release.json --pretty",
    "test": "TEST=true mocha --compilers ts:ts-node/register --exit --timeout 600000 --file \"test/setup.ts\"  --recursive \"test/**/*.spec.ts\"",
    "test:grep": "TEST=true mocha --compilers ts:ts-node/register --exit --timeout 600000 --file \"test/setup.ts\" --recursive",
    "coverage": "nyc npm run test",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "@infuseai/graphql-server": "^1.0.0",
    "@kubernetes/client-node": "^0.5.2",
    "agentkeepalive": "^3.5.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "boom": "^7.2.0",
    "debug": "^3.1.0",
    "js-yaml": "^3.12.0",
    "keycloak-admin": "^1.6.1",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "kubernetes-client": "^5.3.1",
    "lodash": "^4.17.10",
    "minimist": "^1.2.0",
    "moment": "^2.23.0",
    "openid-client": "^2.3.0",
    "pluralize": "^7.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.23",
    "@types/boom": "^7.2.0",
    "@types/chai": "^4.1.2",
    "@types/chai-http": "^3.0.5",
    "@types/debug": "^0.0.30",
    "@types/faker": "^4.1.2",
    "@types/js-yaml": "^3.11.2",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^4.2.1",
