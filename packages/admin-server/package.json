    "json-stream": "^1.0.0",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.3.0",
    "keycloak-admin": "npm:@infuseai/keycloak-admin@1.12.3",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^4.0.0",
    "koa-proxies": "^0.11.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.1.4",
