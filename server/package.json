    "axios": "^0.18.0",
    "basic-auth": "^2.0.1",
    "bluebird": "^3.5.1",
    "boom": "^7.2.0",
    "debug": "^3.1.0",
    "flat": "^4.1.0",
    "graphql": "^0.13.2",
    "graphql-import": "^0.6.0",
    "graphql-middleware": "^1.7.2",
    "graphql-tools": "^3.1.1",
    "graphql-type-json": "^0.2.1",
    "gravatar": "^1.6.0",
    "js-yaml": "^3.12.0",
    "json-stream": "^1.0.0",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.3.0",
    "keycloak-admin": "^1.6.1",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
