{
  "name": "@canner/infuseai-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development ts-node src/bin/www.ts",
    "start:lib": "NODE_ENV=development node lib/bin/www",
    "start:prod": "NODE_ENV=production node lib/bin/www",
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\" \"test/*\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.json --pretty",
    "build:prod": "npm run clean && echo Using TypeScript && tsc --version && tsc -p ./tsconfig.release.json --pretty && npm run copy:views && npm run copy:graphql",
    "copy:views": "cp -R src/views lib/views",
    "copy:graphql": "cp -R src/graphql lib/graphql",
    "test": "TEST=true mocha --compilers ts:ts-node/register --exit --timeout 10000 --file \"test/setup.ts\"  --recursive \"test/**/*.spec.ts\"",
    "test:grep": "TEST=true mocha --compilers ts:ts-node/register --exit --timeout 10000 --file \"test/setup.ts\" --recursive",
    "coverage": "nyc npm run test",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "@kubernetes/client-node": "^0.5.2",
    "agentkeepalive": "^3.5.1",
